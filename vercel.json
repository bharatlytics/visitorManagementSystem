{
    "version": 2,
    "buildCommand": "cd frontend && npm install && npm run build",
    "outputDirectory": "frontend/dist",
    "functions": {
        "run.py": {
            "runtime": "@vercel/python@4.0"
        }
    },
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "/run.py"
        },
        {
            "source": "/auth/:path*",
            "destination": "/run.py"
        },
        {
            "source": "/health",
            "destination": "/run.py"
        }
    ]
}